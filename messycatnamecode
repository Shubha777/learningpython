num = list(range(0, 17))
    catNames=[]
    r=0
    while True:
        r = r+1
        print('Enter the name of your cat numbered  ' + str(num[r]) + ' ( or enter END to stop.):')
        name = input()
        if name == 'END':
            break
        if r==16:
            print('Maximum limit of input has bee achieved. You must not have any more cats')
            break
        catNames = catNames + [name]
        continue
    print('Your list is complete.')
    while True:
        yourpetnames=[]
        print('     ')
        print('Enter your pet name.')
        petname = input()
        print('Your input is ' + petname)
        yourpetnames = yourpetnames + [petname]
        for name in yourpetnames:
            if not petname in catNames:
                print('you dont have a pet named '+ name)
                print('Please try again')
                break  
            elif petname in catNames:
               print('your name is in the list.')
               break
        continue      
